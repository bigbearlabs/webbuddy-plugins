.dev
  %h3 Filter
  %input( ng-model="data.input" ng-change="")

  %h3 Evaluator
  %input( ng-model="dev_input" ng-change="evaluate(dev_input)")
  %p {{dev_output}}



.searches
  %h2 Searches

  %ul.search-list

    %li.item( data-ng-repeat="item in view_model.searches"
      ng-click="click_item(item)"
      )
      %p {{item.name}}

    %li.item
      %p( style="display:none;" ) More...

-# design note: in order to reduce info that has to be processed at a glance, we need to move these entities into interaction results in a coarser-grain entity. Candidates: hanging them off searches or keywords.
.pages
  %h2 Pages

  %ul.page-list(  )

    %li.item( data-ng-repeat="item in view_model.pages"
      )
      %img( ng-src="{{item.thumbnail_url}}")
      %p {{item.name}}



.suggestions( style="display:none;" )
  %h2 Search suggestions for '{{data.input}}'

  %ul.suggestion-list(  )

    %li.item( data-ng-repeat="item in data.suggestions"

      )
      %p {{item}}


.highlights( )
  %h2 Highlighted content for '{{data.input}}'

  %ul.highlight-list(  )

    %li.item( data-ng-repeat="item in data.highlights"

      )
      %p {{item}}


